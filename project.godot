uuid="*res://addons/uuid/uuid.gd"
GlobalMessageManager="*res://systems/chat_and_message_system/scripts/GlobalMessageManager.gd"
GlobalTaskManager="*res://systems/task_system/scripts/GlobalTaskManager.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_resource_groups/plugin.cfg", "res://addons/uuid/plugin.cfg")

