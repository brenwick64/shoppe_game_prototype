config_version=5

[application]

config/name="shoppe_game_prototype"
config/features=PackedStringArray("4.3", "Forward Plus")

[audio]

buses/default_bus_layout="res://audio/default_bus_layout.tres"

[autoload]

GlobalAudioManager="*res://systems/audio_system/scripts/GlobalAudioManager.gd"
DayAndNightManager="*res://systems/day_night_system/scripts/GlobalDayNightManager.gd"
Settings="*res://scripts/globals/settings.gd"
Constants="*res://scripts/globals/constants.gd"
Utils="*res://scripts/globals/utils.gd"
